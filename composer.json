{
    "name": "phumsoft/phumpie",
    "description": "The package is using for custom default laravel package",
    "homepage": "https://github.com/phumsoft/phumpie",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "phumsoft",
        "silak",
        "phumpie",
        "laravel"
    ],
    "autoload": {
        "psr-4": {
            "Phumsoft\\Phumpie\\": "src/"
        },
        "files": [
            "src/Helpers/url.php",
            "src/Helpers/string.php",
            "src/Helpers/header.php",
            "src/Helpers/collection.php",
            "src/Helpers/translation.php"
        ]
    },
    "authors": [
        {
            "name": "Silak",
            "email": "chhumsilak@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/http": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "prettus/laravel-validation": "^1.4"
    },
    "require-dev": {
        "laravel/pint": "^1.2"
    }
}